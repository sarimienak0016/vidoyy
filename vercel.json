{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/e/:id",
      "destination": "/api/proxy/[...slug]"
    },
    {
      "source": "/d/:id", 
      "destination": "/api/proxy/[...slug]"
    },
    {
      "source": "/v/:id",
      "destination": "/api/proxy/[...slug]"
    },
    {
      "source": "/watch/:id",
      "destination": "/api/proxy/[...slug]"
    },
    {
      "source": "/api/proxy/(.*)",
      "destination": "/api/proxy/[...slug]"
    }
  ],
  "functions": {
    "api/proxy/[...slug].js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "devCommand": null,
  "installCommand": "npm install",
  "framework": null
}
