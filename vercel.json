{
  "functions": {
    "api/video.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/e/:id",
      "destination": "/api/video"
    },
    {
      "source": "/d/:id",
      "destination": "/api/video"
    },
    {
      "source": "/v/:id",
      "destination": "/api/video"
    },
    {
      "source": "/",
      "destination": "/api/video"
    }
  ]
}
